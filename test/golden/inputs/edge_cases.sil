---
emptyString: ""
nullValue: null
whitespace: "   "
specialChars: "!@#$%^&*()_+-={}[]|\\:;\"'<>?,./"
unicode: "Hello ‰∏ñÁïå! üåç"
numbers:
  zero: 0
  negative: -42
  float: -3.14159
  large: 999999999
emptyList: []
emptyObject: {}
nested:
  empty:
    list: []
    object: {}
    string: ""
  mixed:
    - ""
    - null
    - 0
    - false
    - {}
    - []
mixedTypes:
  - true
  - 42
  - "string"
  - 3.14
  - null
  - {}
  - []
deeply:
  nested:
    structure:
      with:
        empty:
          values: null
        and:
          lists: []
          objects: {}
quotes:
  single: "it's working"
  double: 'she said "hello"'
  both: "it's a \"test\""
  escaped: "line1\nline2\ttab"
booleans:
  trueVal: true
  falseVal: false
---

